  script.
    if(local_authmessage!=""){
      document.getElementById("loginbutton").click();
    }
    if(local_ingreso.length !=0){
      var myModal = new bootstrap.Modal(document.getElementById('ModalInformacion'), {
        keyboard: false
      })
      myModal.toggle()
    }

    window.onscroll = function() {myFunction()};

    var header = document.getElementById("blur1");
    var sticky = header.offsetTop;
    document.getElementById("aceptarusuario").disabled = true

    function myFunction() {
      if (window.pageYOffset > sticky) {
        header.classList.add("sticky");
      } else {
        header.classList.remove("sticky");
      }
    }