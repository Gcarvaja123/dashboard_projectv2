doctype html
html
  head
    
    link(rel='stylesheet' type='text/css' href='/css/login.css')
  body

    
    .container-login100
      .wrap-login100.p-t-85.p-b-20
        form.login100-form.validate-form(action='/login' method='post' enctype='multipart/form-data')
          span.login100-form-title.p-b-70
            | Bienvenido
          span.login100-form-avatar
            img.imagen3()
          .wrap-input100.validate-input.m-t-85.m-b-35(data-validate='Enter username')
            input.input100(type='text' name='username' placeholder='Usuario' )
          .wrap-input100.validate-input.m-b-50(data-validate='Enter password')
            input.input100(type='password' name='password'  placeholder='Contrase√±a')
          .container-login100-form-btn
            button.login100-form-btn
              | Ingresar
